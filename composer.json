{
    "name":        "cvsoft/yii2-profiler",
    "description": "Actions profiler for YII2 applications",
    "type":        "yii2-extension",
    "license":     "BSD3-Clause",
    "authors": [
        {
            "name":  "Alexey Volkov",
            "email": "webwizardry@hotmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">7.0",
        "yiisoft/yii2": "~2.0.15.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.4"
    },
    "autoload": {
        "psr-4": {
            "cvsoft\\profiler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "app\\": "src-test"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
